document.getElementsByClassName("video-header-tab-a")[1].setAttribute("href",document.getElementById("video_player").src);
function clickC(answer,url){
 for(i=0;i<=3;i++){
   if(document.getElementsByClassName("border-btn")[i].innerHTML == answer){
      document.getElementsByClassName("border-btn")[i].click();
      setTimeout(function(){document.getElementById("nextvideo").click();},1000);
   }
 }
}
setInterval(function(){
  if(document.getElementsByClassName("themeBtn")[0].innerHTML != "Next Video"){
    document.getElementsByClassName("themeBtn")[0].click();
  }
  if(document.getElementsByClassName("video-page-current-duration")[0].innerHTML == document.getElementsByClassName("video-page-total-duration")[0].innerHTML){
    clickC("Vlog","https://www.youtube.com/embed/q3y4gUm9OjA?enablejsapi=1&playsinline=1&autoplay=false&controls=0&rel=0&showinfo=0&egm=0&showsearch=0&modestbranding=1&iv_load_policy=3&disablekb=1&loop=0&start=0");
    clickC("None","https://www.youtube.com/embed/q3y4gUm9OjA?enablejsapi=1&playsinline=1&autoplay=false&controls=0&rel=0&showinfo=0&egm=0&showsearch=0&modestbranding=1&iv_load_policy=3&disablekb=1&loop=0&start=0");
    
    clickC("Pop","https://www.youtube.com/embed/mVdjnzO_X8U?enablejsapi=1&playsinline=1&autoplay=false&controls=0&rel=0&showinfo=0&egm=0&showsearch=0&modestbranding=1&iv_load_policy=3&disablekb=1&loop=0&start=0");
    
    clickC("Rock","https://www.youtube.com/embed/n_3dZqqz4hk?enablejsapi=1&playsinline=1&autoplay=false&controls=0&rel=0&showinfo=0&egm=0&showsearch=0&modestbranding=1&iv_load_policy=3&disablekb=1&loop=0&start=0");
    
